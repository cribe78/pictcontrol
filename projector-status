<?php

include("controller-lib.php");


$projectors = array();


for ($i = 1; $i <= 5; $i++) {
    $projectors[$i]["power"] = f32Command($i, "POWR", "?");
    echo "P$i power: {$projectors[$i]["power"]}\n";
    $projectors[$i]["serial"] = f3CCommand($i, "SERI", "?");
    echo "P$i serial: {$projectors[$i]["serial"]}\n";
}
